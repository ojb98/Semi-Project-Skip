<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="rental.mapper.ItemCategoryMapper">
    <select id="selectItemCategoryById" parameterType="String" resultType="rental.dto.ItemCategoryDTO">
        select * from item_category where item_type = #{item}
    </select>

    <select id="getList" resultType="arraylist">
        select * from item_category
    </select>

</mapper>

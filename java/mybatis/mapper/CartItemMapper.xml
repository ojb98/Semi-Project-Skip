<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mybatis.mapper.CartItemMapper">
    <insert id="insertCartItem" parameterType="dto.CartItemDTO">
        insert into cart_items values (cart_items_seq.nextval, #{uuid}, #{category}, #{ref_id}, #{quantity}, #{price})
    </insert>
</mapper>
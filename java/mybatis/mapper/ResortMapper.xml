<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mybatis.mapper.ResortMapper">
    <select id="selectResortById" parameterType="int" resultType="dto.ResortDTO">
        select * from resort where resort_id = #{resort_id}
    </select>

    <select id="getList" resultType="dto.ResortDTO">
        select * from resort
    </select>

</mapper>
